{{#with stream}}
<tr class="default_stream_row" id="{{name}}">
  <td>
      {{#if invite_only}}<i class="icon-vector-lock "></i>{{/if}}
      <span class="default_stream_name">{{name}}</span>
  </td>
  <td>
      <button class="button remove-default-stream btn-danger">
        {{t "Remove from default" }}
      </button>
  </td>
</tr>
{{/with}}
